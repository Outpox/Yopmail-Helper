{
	"manifest_version": 2,

	"name": "Yopmail Helper",
	"description": "This extension helps you with your yopmail adresses.",
	"version": "1.6",
	"icons": {
		"48": "img/icon.png"
	},
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_popup": "popup.html"
		},
	"permissions": [
		"storage"
	],
	"background": {
		"scripts":  ["background.js"]
	},
	"default_locale": "en"
}